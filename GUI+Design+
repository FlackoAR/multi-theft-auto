local myWindow = guiCreateWindow ( 0, 0, 0.5, 0.4, "UDC - Supporter Window", true ) 
local tabPanel = guiCreateTabPanel ( 0, 0.1, 1, 1, true, myWindow )      
local tabMap = guiCreateTab( "Player Info", tabPanel )                
local tabHelp = guiCreateTab( "Help", tabPanel )                          


guiCreateLabel(0.02, 0.04, 0.94, 0.2, "UDC - Guide", true, tabMap)
guiCreateLabel(0.02, 0.04, 0.94, 0.92, "This is help text.", true, tabHelp)